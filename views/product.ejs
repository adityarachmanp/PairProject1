<body>
  <a href="/products/add">Add</a>
	<h1>Product</h1>
	<table class = "display" id="tabel_id">
		<thead>
			<tr>
				<th>Id</th>
				<th>Name</th>
				<th>Qty</th>
				<th>Price</th>
        <th>Action</th>
			</tr>
		</thead>
		<tbody>
		<%	data.forEach(el => { %>
				<tr>
					<td><%= el.id %></td>
					<td><%= el.name %></td>
					<td><%= el.qty %></td>
          <td><%= el.price %></td>
          <td>
            <a href="/products/update/<%= el.id %>">Update</a> |
            <a href="/products/delete/<%= el.id %>">Delete</a>
          </td>
				</tr>
		<%	}) %>
		</tbody>
  </table>
